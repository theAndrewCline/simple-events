{
  "name": "simple-services",
  "version": "1.0.0",
  "description": "stupid simple planning center replacement",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "dev": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "test": "ava --watch",
    "start:client": "parcel ./client/src/index.html -d client/dist",
    "start:server": "concurrently \"npm run server:bundle\" \"nodemon ./server/dist\"",
    "server:bundle": "parcel watch ./server/src/index.ts --target node -d server/dist",
    "prod": "npm run client:prod && npm run server:prod",
    "client:prod": "parcel build ./client/src/index.html -d client/dist",
    "server:prod": "parcel build ./server/src/index.ts -d server/dist",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theAndrewCline/simple-services.git"
  },
  "keywords": [
    "functional",
    "react",
    "node"
  ],
  "author": "Andrew Cline",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theAndrewCline/simple-services/issues"
  },
  "homepage": "https://github.com/theAndrewCline/simple-services#readme",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "ava": "^2.4.0",
    "chai": "^4.2.0",
    "concurrently": "^5.0.0",
    "cypress": "^3.6.1",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^1.19.4",
    "parcel": "^1.12.4",
    "ts-node": "^8.5.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "immer": "^5.0.0",
    "ramda": "^0.26.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}